<%!
char* upper(char* in) {
	size_t len = strlen(in);
	char* result = kore_malloc(len);
	memcpy(result, in, len);
	for (char* i = result; *i; i++) {
		*i &= ~32;
	}
	return result;
}
%>
<!doctype html>
<html>
	<body>
		<%= upper("test") %>
		<%// comment %>
		<br />
		<% for (int i = 0; i < 10; i++) { %>
			Hello, world <%= i %><br />
		<% } %>
	</body>
</html>
